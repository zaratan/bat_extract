{
  "name": "bat_extract",
  "version": "1.0.0",
  "description": "Extracteur de données d'images utilisant l'OCR avec TypeScript",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:demo": "nodemon --exec ts-node src/demo.ts",
    "dev:departments": "nodemon --exec ts-node src/extractDepartments.ts",
    "dev:analyze": "nodemon --exec ts-node src/analyzeCarte.ts",
    "dev:distribution": "nodemon --exec ts-node src/extractDistribution.ts",
    "dev:compare": "nodemon --exec ts-node src/compareMaps.ts",
    "dev:smart": "nodemon --exec ts-node src/smartExtractor.ts",
    "dev:tagger": "ts-node src/departmentTagger.ts",
    "dev:color-tagger": "ts-node src/colorLegendTagger.ts",
    "dev:simple-color": "ts-node src/simpleColorTagger.ts",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postbuild": "echo 'Build terminé!'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ocr",
    "image",
    "extraction",
    "typescript",
    "tesseract"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "fs-extra": "^11.3.1",
    "sharp": "^0.34.3",
    "tesseract.js": "^6.0.1"
  }
}
